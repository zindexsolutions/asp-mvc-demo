
@{
    ViewBag.Title = "404 Page Not Found";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


    <div class="error-page">
        <h2 class="headline text-blue"> 404</h2>

        <div class="error-content">
            <h3><i class="fa fa-warning text-blue"></i> Oops! Page not found.</h3>

            <p>
                We could not find the page you were looking for.
                Meanwhile, you may <a href="@Url.Action("Index","Home")">return to dashboard</a> or try using the search form.
            </p>

            
        </div>
        <!-- /.error-content -->
    </div>
    <!-- /.error-page -->